<div class="d-flex flex-column justify-content-center align-items-center border-bottom mb-5">
    <h3 class="text-black-50">TEAM LEADER</h3>
    <div class="row">
        <% teamLeaders.forEach(item => { %>
          <div class="d-flex flex-column align-items-center col-12 col-md-6 col-lg-3 p-5">
              <div class="mt-4 mb-4 card border-0" style="width: 14rem">
                  <div class="overlay card">
                    <div class="item1 d-flex flex-row align-items-center position-relative overflow-hidden" style="height: 15rem">
                      <img src="<%= item.avatar %>">
                      <div class="content">
                        <ul class="list">
                          <li>
                            <a href="#">
                              <i class="fab fa-twitter"></i>
                            </a>
                           </li>
                          <li>
                            <a href="#">
                              <i class="fab fa-facebook-f"></i>
                            </a>
                          </li>
                          <li>
                            <a href="#">
                              <i class="fab fa-linkedin-in"></i>
                            </a>
                          </li>
                          <li>
                            <a href="#">
                              <i class="fas fa-camera-retro"></i>
                            </a>
                          </li>
                        </ul>
                      </div>
                    </div>
                    <div class="card-body d-flex flex-column align-items-center">
                      <div class="card-title">
                        <a href="#"><%= item.name %></a>
                      </div>
                      <div class="card-text" style="font-size: 20px">
                        <%= item.job %>
                      </div>
                    </div>
                  </div>
              </div>
          </div>  
        <% }) %>         
    </div>
</div>